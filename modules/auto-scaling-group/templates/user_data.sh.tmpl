#!/bin/bash

mkdir ~/.ssh
chmod 700 ~/.ssh
touch ~/.ssh/authorized_keys
chmod 600 ~/.ssh/authorized_keys
echo "ssh-rsa  rss@testing" >> ~/.ssh/authorized_keys
apt-get install -y redis-cli


FILE=${file_name}

/usr/local/bin/aws s3 cp s3://${bucket_name}/$FILE /tmp/
cd /tmp && tar xzf $FILE
/tmp/run.sh
